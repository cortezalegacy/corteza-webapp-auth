{
  "name": "webapp-auth",
  "description": "Crust Authentication WebApp",
  "private": true,
  "version": "2019.3.1",
  "license": "Apache-2.0",
  "contributors": [
    "Denis Arh <denis.arh@gmail.com>"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode production",
    "lint": "vue-cli-service lint"
  },
  "gitHooks": {
    "pre-commit": "yarn lint"
  },
  "stylelint": {
    "plugins": [
      "stylelint-scss"
    ],
    "extends": "stylelint-config-standard",
    "rules": {
      "color-hex-case": null,
      "color-hex-length": null,
      "no-empty-source": null,
      "selector-list-comma-newline-after": null
    }
  },
  "dependencies": {
    "@vue/eslint-config-standard": "^4.0.0",
    "core-js": "^2.6.5",
    "stylelint": "^9.10.1",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.5.4",
    "stylelint-webpack-plugin": "^0.10.5",
    "crust-api-client.js": "crusttech/crust-api-client.js.git#master",
    "crust-common.vue": "crusttech/crust-common.vue.git#master",
    "vue": "^2.6.6",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
